{
  "name": "@astroshibapop/api-gateway-v2",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist .turbo",
    "deploy:check": "./scripts/pre-deploy-check.sh",
    "deploy:preview": "vercel",
    "deploy:prod": "vercel --prod",
    "deploy:verify": "./scripts/verify-deployment.sh"
  },
  "dependencies": {
    "@astroshibapop/shared": "workspace:*",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/sensible": "^6.0.1",
    "@prisma/client": "5.22.0",
    "@vercel/kv": "^3.0.0",
    "close-with-grace": "^2.1.0",
    "dataloader": "^2.2.3",
    "dotenv": "^16.3.1",
    "fastify": "^5.2.0",
    "graphql": "^16.9.0",
    "graphql-query-complexity": "^1.1.0",
    "ioredis": "^5.3.2",
    "mercurius": "^15.0.0",
    "mercurius-integration-testing": "^8.2.0",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@vercel/node": "^3.0.21",
    "@vitest/ui": "2.1.9",
    "c8": "^10.1.3",
    "prisma": "5.22.0",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "2.1.9"
  }
