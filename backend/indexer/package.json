    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@astroshibapop/shared-types": "workspace:*",
    "@prisma/client": "^5.8.0",
    "@stellar/stellar-sdk": "^11.2.2",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "p-queue": "^9.0.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prisma": "^5.8.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
